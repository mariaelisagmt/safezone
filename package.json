    "@angular/forms": "^20.0.0",
    "@angular/material": "^20.0.2",
    "@angular/platform-browser": "^20.0.0",
    "@angular/router": "^20.0.0",
    "@fortawesome/fontawesome-free": "^6.7.2",
    "leaflet": "^1.9.4",
    "leaflet-control-geocoder": "^3.2.0",
    "leaflet.heat": "^0.2.0",
    "ngx-leaflet": "^0.0.16",
