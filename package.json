    "jest": "^29.7.0",
    "jest-preset-angular": "^14.6.0",
    "json-server": "^1.0.0-beta.3",
    "prettier": "^3.5.3",
    "typescript": "~5.8.2",
    "typescript-eslint": "8.33.1"
